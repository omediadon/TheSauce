{
	"name"        : "dondev/thesauce",
	"description" : "Simple skeleton for slim 4",
	"keywords"    : [
		"template",
		"dondev",
		"slim",
		"skeleton"
	],
	"homepage"    : "https://dondev.ga/",
	"license"     : "LGPLv3",
	"authors"     : [
		{
			"name" : "DonDev",
			"email": "admin@memze.ga"
		}
	],
	"require"     : {
		"php"                   : ">=7.4",
		"ext-json"              : "*",
		"ext-pdo"               : "*",
		"bryanjhv/slim-session" : "4.0.3",
		"cakephp/chronos"       : "2.0.5",
		"cakephp/database"      : "4.0.8",
		"lcobucci/jwt"          : "3.3.2",
		"monolog/monolog"       : "^2.0",
		"php-di/php-di"         : "^6.1",
		"phpmailer/phpmailer"   : "v6.1.6",
		"ramsey/uuid"           : "4.0.1",
		"selective/array-reader": "1.0.0",
		"selective/validation"  : "0.10.0",
		"slim/csrf"             : "1.0.0",
		"slim/http"             : "1.0.0",
		"slim/http-cache"       : "0.4.0",
		"slim/php-view"         : "2.2.1",
		"slim/psr7"             : "^1.1",
		"slim/slim"             : "^4.5",
		"slim/twig-view"        : "3.1.0"
	},
	"require-dev" : {
		"roave/security-advisories": "dev-master"
	},
	"config"      : {
		"process-timeout": 0,
		"sort-packages"  : true
	},
	"autoload"    : {
		"psr-4": {
			"App\\"   : "src/app/",
			"System\\": "src/sys/"
		}
	},
	"autoload-dev": {
		"psr-4": {
		}
	},
	"scripts"     : {
		"start": "php -S localhost:8080 -t public"
	}
}
